# Master Server
MASTER_PORT=3100
JWT_SECRET=your-jwt-secret-here
DASHBOARD_SECRET=your-dashboard-secret-here

# TLS (optional - leave empty for HTTP)
TLS_CERT_PATH=
TLS_KEY_PATH=

# Dashboard
DASHBOARD_PORT=3200
DASHBOARD_USER=admin
DASHBOARD_PASSWORD=changeme
MASTER_URL=http://localhost:3100

# Session Persistence (default: true when omitted)
SESSION_PERSISTENCE=true

# Node Agent
NODE_ID=my-node
NODE_NAME=My Machine
MASTER_URL=http://localhost:3100
NODE_TOKEN=your-jwt-token-here
CLAUDE_MODEL=claude-sonnet-4-5-20250929
CLAUDE_SKIP_PERMISSIONS=false
ALLOWED_PATHS=/home/user/projects
DENIED_COMMANDS=rm -rf /,shutdown,reboot,mkfs
DEFAULT_SHELL=/bin/bash
MAX_QUEUE_SIZE=10
MAX_TASK_TIMEOUT_MS=300000

# MCP Server
MCP_NODE_ID=my-node
MCP_MASTER_URL=http://localhost:3100
MCP_TOKEN=your-jwt-token-here
